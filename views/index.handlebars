	
	<meta property="og:type" content="website">
	<meta property="og:url" content="https://labyaddons.de/">
	<meta property="og:title" content="labyaddons.de - The LabyMod Addon List">
	<meta property="og:description" content="This is a list of all offical and inoffical LabyMod Addons.">
	<meta property="og:image" content="https://i.imgur.com/SDbRq5U.png">

<link rel="stylesheet" href="./static/css/btn.css">
<link rel="stylesheet" href="./static/css/index.css">

<div id="message" class="slideIn hidden" > 
		<img src="./static/images/Alertbox.png" alt="alert" class="bg">
		<div class="content">
			<h3 id="title">Title</h3>
			<p id="description">Description</p>
		</div>
</div>

	{{#if user}}
		<div class="user-area">
			<div class="bg">
				{{#each tabs}}
					<span class="link"><a href="{{this.link}}">{{this.text}}</a></span>
				{{/each}}
				<span class="username">{{user.displayName}}</span>
				<a href="{{user.profileUrl}}" class="profile-link"><img src="{{photo}}" alt="{{user.displayName}}"></a>
			</div>
			<span class="link"><a href="logout"><i class="fas fa-sign-out-alt"></i> Logout</a></span>
		</div>
		{{else}}
		<div class="login-btn">
			<button class="btn login" id="login" onclick="redirect('/auth/github');"><span>Login with </span> <i class="fab fa-github"></i></button>
		</div>
	{{/if}}

<div class="wrapper-addon">
	<div class="wrapper">
		<div class="tab">
			<button class="tablinks" onclick="openTab(event, 'store_offical')" id="offical">Offical Store</button>
			<button class="tablinks" onclick="openTab(event, 'store_inoffical')" id="inoffical">Inoffical Store</button>
		</div>
			<!-- Tab content -->
			<div data-to="offical" id="store_offical" class="tabcontent">
					<div class="center">
						<input type="text" autocomplete="off" id="offical-search" class="search" placeholder="Search..."/>
						<div class="select">
						<select id="offical-version">
							<option selected value="18">1.8</option>
							<option value="112">1.12</option>
							<option value="116">1.16</option>
						</select>
						<div class="select_arrow">
					</div>
					</div>
					<button class="btn share" id="share" onclick="share();"><i class="fas fa-share-alt"></i></button>
				</div>
				<div class="center time"><p class="lastfetch">TIME</p></div>
								
				<div class="center">
					<div id="offical-addons" class="addons"></div>
				</div>
			</div>
			<div data-to="inoffical" id="store_inoffical" class="tabcontent">
					<div class="center">
						<input type="text" autocomplete="off" id="inoffical-search" class="search" placeholder="Search..."/>
						<div class="select">
							<select id="inoffical-version">
								<option selected value="18">1.8</option>
								<option value="112">1.12</option>
								<option value="116">1.16</option>
							</select>
							<div class="select_arrow">
							</div>
					</div>
					<button class="btn share" id="share" onclick="share();"><i class="fas fa-share-alt"></i></button>
					</div>
					<div class="center time"><p class="lastfetch">TIME</p></div>
					<div class="center">
						<div id="inoffical-addons"  class="addons"></div>
					</div>
			</div>
	</div>



</div>

<div id="links">
	<a href="imprint">Imprint</a> /
	<a href="privacy">Privacy Policy</a>
</div>


<template id="addon-template">
	<div class="addon-card">
	<img id="icon" src="#" alt="NAME" class="addon-img">
	<div class="addon-text">
		<h2 id="name">ADDON NAME</h2>
		<p id="author">ADDON AUTHOR</p>
		<p id="description">Lorem ipsum dolor sit amet consectetur adipisicing elit. Voluptate incidunt recusandae at? Est odit suscipit consequuntur? Dignissimos aut assumenda tempore, a perspiciatis odit quas voluptatum optio dicta, ex tempora doloremque.</p>
	</div>  
	<div class="vertical-center">
		<button class="btn download" id="download" onclick="download(this);"><i class="fas fa-download"></i></button>
	</div>
</template>

<script type="text/javascript">
    function redirect(url){
 		location.href = url;
	}
</script>


<script src="./static/js/app.js"></script>
